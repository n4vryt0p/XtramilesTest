@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <div class="row">
        <select class="form-control" id="countryEx" name="Abbreviation" asp-items="@Model.Countrieses" style="width: 50%">
          <option value=""></option>
        </select>
    </div>
    
    <div class="row mt-3">
    <select class="form-control" id="cityEx" disabled style="width: 50%">
      <option value=""></option>
    </select>
    </div>
    
    <div class="row mt-3">
        <label>Latitude: </label>
        <input class="form-control" type="text" id="lat"/>
    </div>
    <div class="row mt-3">
        <label>Longitude: </label>
        <input class="form-control" type="text" id="long"/>
    </div>
    <div class="row mt-3">
        <label>Time: </label>
        <input class="form-control" type="text" id="time"/>
    </div>
    <div class="row mt-3">
        <label>Wind Speed: </label>
        <input class="form-control" type="text" id="wSpeed"/>
    </div>
    <div class="row mt-3">
        <label>Weather: </label>
        <input class="form-control" type="text" id="weather"/>
    </div>
    <div class="row mt-3">
        <label>Tempertaure: </label>
        <input class="form-control" type="text" id="tempF"/>
        <input class="form-control" type="text" id="tempC"/>
    </div>
</div>
